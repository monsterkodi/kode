Kode = require '../kode'
kode = -> new Kode()

module.exports = 
            
    ast:(c,p) -> kode().astr c, no
    ke: (c)   -> kode().eval(c)
    kc: (c,p) -> 
        k = kode().compile c
        if k.startsWith '// monsterkodi/kode'   then k = k[k.indexOf('\n')+2..]
        if k.startsWith 'var _k_'               then k = k[k.indexOf('\n')+2..]
        if k.startsWith 'var '                  then k = k[k.indexOf('\n')+2..]
        k
        